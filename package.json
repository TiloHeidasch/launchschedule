{
  "name": "schedule",
  "version": "1.0.0",
  "author": "Tilo Heidasch",
  "homepage": "https://launchschedule.th105.de/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ionic build --prod",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:coverage": "ng test --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --webdriver-update false",
    "pree2e": "node_modules/protractor/bin/webdriver-manager update --versions.chrome=89.0.4389.114 --gecko false --standalone false",
    "screenshots": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/pixel2xlprotractor.conf.js",
    "screenshots:pixel2xl": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/pixel2xl.protractor.conf.js",
    "screenshots:nexus7": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/nexus7.protractor.conf.js",
    "screenshots:nexus10": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/nexus10.protractor.conf.js",
    "resources": "cordova-res android && node scripts/resources.js",
    "android": "ionic build && npx cap copy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.2",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/service-worker": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@angular/youtube-player": "^11.2.13",
    "@capacitor/android": "^2.4.7",
    "@capacitor/core": "2.4.7",
    "@capacitor/ios": "^2.4.7",
    "@ionic-native/core": "^5.33.1",
    "@ionic-native/http": "^5.33.1",
    "@ionic-native/splash-screen": "^5.33.1",
    "@ionic-native/status-bar": "^5.33.1",
    "@ionic/angular": "^5.6.8",
    "@ionic/core": "^5.6.8",
    "@types/leaflet": "^1.7.0",
    "chart.js": "^2.9.4",
    "codelyzer": "^6.0.2",
    "cordova-plugin-advanced-http": "^3.1.0",
    "cordova-plugin-file": "^6.0.2",
    "leaflet": "^1.7.1",
    "ngx-ionic-image-viewer": "^0.7.4",
    "primeicons": "^4.1.0",
    "primeng": "^11.4.2",
    "rxjs": "~6.6.7",
    "ts-md5": "^1.2.8",
    "tslib": "^2.2.0",
    "uuidv4": "^6.2.8",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.2",
    "@angular/cli": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@angular/language-service": "~12.0.2",
    "@capacitor/cli": "3.0.2",
    "@ionic/angular-toolkit": "^3.1.1",
    "@types/jasmine": "~3.7.6",
    "@types/jasminewd2": "~2.0.9",
    "@types/node": "^15.6.1",
    "cordova-res": "^0.15.3",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "puppeteer": "^10.0.0",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.2.4"
  },
  "description": "App for upcoming rocket launches"
}
