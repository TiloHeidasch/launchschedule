    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@angular/youtube-player": "^14.1.0",
    "@capacitor-community/fcm": "^2.0.2",
    "@capacitor/android": "^3.6.0",
