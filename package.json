{
  "name": "schedule",
  "version": "1.0.0",
  "author": "Tilo Heidasch",
  "homepage": "https://launchschedule.th105.de/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ionic build --prod",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:coverage": "ng test --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --webdriver-update false",
    "pree2e": "node_modules/protractor/bin/webdriver-manager update --versions.chrome=89.0.4389.114 --gecko false --standalone false",
    "screenshots": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/pixel2xlprotractor.conf.js",
    "screenshots:pixel2xl": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/pixel2xl.protractor.conf.js",
    "screenshots:nexus7": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/nexus7.protractor.conf.js",
    "screenshots:nexus10": "npm run pree2e && ng e2e --webdriver-update false --protractor-config screenshots/nexus10.protractor.conf.js",
    "resources": "cordova-res android && node scripts/resources.js",
    "android": "ionic build && npx cap copy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.7",
    "@angular/cdk": "^11.2.11",
    "@angular/common": "~11.1.1",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.1.1",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/service-worker": "~11.2.11",
    "@angular/router": "~11.2.7",
    "@angular/youtube-player": "^11.2.11",
    "@capacitor/android": "^2.4.7",
    "@capacitor/core": "2.4.7",
    "@capacitor/ios": "^2.4.7",
    "@ionic-native/core": "^5.32.1",
    "@ionic-native/http": "^5.32.1",
    "@ionic-native/splash-screen": "^5.32.1",
    "@ionic-native/status-bar": "^5.32.1",
    "@ionic/angular": "^5.6.6",
    "@ionic/core": "^5.6.6",
    "@types/leaflet": "^1.7.0",
    "chart.js": "^2.9.4",
    "codelyzer": "^6.0.1",
    "cordova-plugin-advanced-http": "^3.1.0",
    "cordova-plugin-file": "^6.0.2",
    "leaflet": "^1.7.1",
    "ngx-ionic-image-viewer": "^0.7.4",
    "primeicons": "^4.1.0",
    "primeng": "^11.4.0",
    "rxjs": "~6.6.7",
    "ts-md5": "^1.2.7",
    "tslib": "^2.2.0",
    "uuidv4": "^6.2.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.11",
    "@angular/cli": "~11.2.11",
    "@angular/compiler": "~11.1.1",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.2.12",
    "@capacitor/cli": "2.4.7",
    "@ionic/angular-toolkit": "^3.1.1",
    "@types/jasmine": "~3.6.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^15.0.1",
    "cordova-res": "^0.15.3",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "puppeteer": "^9.1.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.5"
  },
  "description": "App for upcoming rocket launches"
}
